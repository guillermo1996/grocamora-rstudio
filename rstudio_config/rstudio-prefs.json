{
    "wrap_tab_navigation": false,
    "save_workspace": "never",
    "load_workspace": false,
    "initial_working_directory": "~",
    "show_last_dot_value": true,
    "always_save_history": false,
    "auto_detect_indentation": true,
    "continue_comments_on_newline": true,
    "scroll_past_end_of_document": true,
    "highlight_r_function_calls": true,
    "rainbow_parentheses": true,
    "auto_append_newline": true,
    "strip_trailing_whitespace": true,
    "check_unexpected_assignment_in_function_call": true,
    "show_diagnostics_cpp": false,
    "syntax_color_console": true,
    "console_double_click_select": true,
    "panes": {
        "quadrants": [
            "Source",
            "Console",
            "TabSet1",
            "TabSet2"
        ],
        "tabSet1": [
            "Environment",
            "History",
            "Build",
            "VCS",
            "Presentation"
        ],
        "tabSet2": [
            "Files",
            "Plots",
            "Packages",
            "Help",
            "Viewer",
            "Presentations"
        ],
        "hiddenTabSet": [
            "Connections",
            "Tutorial"
        ],
        "console_left_on_top": false,
        "console_right_on_top": true,
        "additional_source_columns": 0
    },
    "save_files_before_build": true,
    "show_rmd_render_command": true,
    "spelling_dictionary_language": "en_GB",
    "posix_terminal_shell": "bash",
    "terminal_bell_style": "none",
    "default_project_location": "~/grocamora/RytenLab-Research",
    "source_with_echo": true,
    "indent_guides": "gray",
    "editor_theme": "Tomorrow Night Bright",
    "rmd_preferred_template_path": "/usr/local/lib/R/site-library/grpRmdTemplate/rmarkdown/templates/basic_markdown"
}